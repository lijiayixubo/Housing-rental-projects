                              @keyup.enter.native=\"handleQuery\"\n                            />\n                        </el-form-item>\n                        <el-form-item label=\"锁状态\" prop=\"status\" class=\"el-form-search-item\">\n                            <el-select v-model=\"queryParams.status\" placeholder=\"请选择锁状态\" clearable size=\"mini\">\n                                <el-option\n                                        v-for=\"dict in dict.type.house_auth_status\"\n                                        :key=\"dict.value\"\n                                        :label=\"dict.label\"\n                                        :value=\"dict.value\"\n                                />\n                            </el-select>\n                        </el-form-item>\n                        <el-form-item label=\"锁密码\" prop=\"password\" class=\"el-form-search-item\">\n                            <el-input\n                                    v-model=\"queryParams.password\"\n                                    placeholder=\"请输入锁密码\"\n                                    clearable\n                                    size=\"mini\"\n                                    @keyup.enter.native=\"handleQuery\"\n                            />\n                        </el-form-item>\n                        <el-form-item label=\"账号\" prop=\"account\" class=\"el-form-search-item\">\n                            <el-input\n                                    v-model=\"queryParams.account\"\n                                    placeholder=\"请输入账号\"\n                                    clearable\n                                    size=\"mini\"\n                                    @keyup.enter.native=\"handleQuery\"\n                            />\n                        </el-form-item>\n            <el-form-item class=\"el-form-search-item\">\n                <el-button type=\"primary\" icon=\"el-icon-search\" size=\"mini\" @click=\"handleQuery\">搜索</el-button>\n                <el-button icon=\"el-icon-refresh\" size=\"mini\" @click=\"resetQuery\">重置</el-button>\n            </el-form-item>\n        </el-form>\n    </el-row>\n\n    <el-table :height=\"tableHeight\" v-loading=\"loading\" :data=\"lockList\" @selection-change=\"handleSelectionChange\">\n      <el-table-column type=\"selection\" width=\"55\" align=\"center\" />\n        <el-table-column label=\"#\" type=\"index\" width=\"50\" align=\"center\">\n            <template scope=\"scope\">\n          <span>{{\n            (queryParams.pageNum - 1) * queryParams.pageSize + scope.$index + 1\n          }}</span>\n            </template>\n        </el-table-column>\n      <el-table-column label=\"房间编号\" align=\"center\" prop=\"houseId\" />\n      <el-table-column label=\"锁ID\" align=\"center\" prop=\"lockId\" />\n      <el-table-column label=\"锁状态\" align=\"center\" prop=\"status\">\n        <template slot-scope=\"scope\">\n          <dict-tag :options=\"dict.type.house_auth_status\" :value=\"scope.row.status\"/>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"锁密码\" align=\"center\" prop=\"password\" />\n      <el-table-column label=\"账号\" align=\"center\" prop=\"account\" />\n      <el-table-column label=\"操作\" align=\"center\" class-name=\"small-padding fixed-width\" width=\"150\">\n        <template slot-scope=\"scope\">\n          <el-button\n            size=\"mini\"\n            type=\"text\"\n            icon=\"el-icon-edit\"\n            @click=\"handleUpdate(scope.row)\"\n            v-hasPermi=\"['system:lock:edit']\"\n          >修改</el-button>\n          <el-button\n            size=\"mini\"\n            type=\"text\"\n            icon=\"el-icon-delete\"\n            @click=\"handleDelete(scope.row)\"\n            v-hasPermi=\"['system:lock:remove']\"\n          >删除</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <pagination\n      v-show=\"total>0\"\n      :total=\"total\"\n      :page.sync=\"queryParams.pageNum\"\n      :limit.sync=\"queryParams.pageSize\"\n      @pagination=\"getList\"\n    />\n\n    <!-- 添加或修改锁单对话框 -->\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"800px\" append-to-body :close-on-click-modal=\"false\" v-dialogDrag>\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\n        <el-form-item label=\"#\" prop=\"id\">\n          <el-input v-model=\"form.id\" placeholder=\"请输入#\" />\n        </el-form-item>\n        <el-form-item label=\"锁ID\" prop=\"lockId\">\n          <el-input v-model=\"form.lockId\" placeholder=\"请输入锁ID\" />\n        </el-form-item>\n        <el-form-item label=\"锁状态\">\n          <el-radio-group v-model=\"form.status\">\n            <el-radio\n              v-for=\"dict in dict.type.house_auth_status\"\n              :key=\"dict.value\"\n:label=\"parseInt(dict.value)\"\n            >{{dict.label}}</el-radio>\n          </el-radio-group>\n        </el-form-item>\n        <el-form-item label=\"锁密码\" prop=\"password\">\n          <el-input v-model=\"form.password\" placeholder=\"请输入锁密码\" />\n        </el-form-item>\n        <el-form-item label=\"账号\" prop=\"account\">\n          <el-input v-model=\"form.account\" placeholder=\"请输入账号\" />\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\n        <el-button @click=\"cancel\">取 消</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport { listLock, getLock, delLock, addLock, updateLock } from \"@/api/system/lock\";\n\nexport default {\n  name: \"Lock\",\n  dicts: ['house_auth_status'],\n  data() {\n    return {\n      // 表格高度\n      tableHeight: document.documentElement.clientHeight - 180,\n      // 遮罩层\n      loading: true,\n      // 选中数组\n      ids: [],\n      // 非单个禁用\n      single: true,\n      // 非多个禁用\n      multiple: true,\n      // 显示搜索条件\n      showSearch: true,\n    