it.sync=\"queryParams.pageSize\"\n      @pagination=\"getList\"\n    />\n\n    <!-- 添加或修改锁单对话框 -->\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"800px\" append-to-body :close-on-click-modal=\"false\" v-dialogDrag>\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\n        <el-form-item label=\"房间编号\">\n          <el-autocomplete\n            popper-class=\"my-autocomplete\"\n            v-model=\"form.houseId\"\n            :fetch-suggestions=\"querySearch\"\n            placeholder=\"输入要搜索的房间编号或名称\">\n            <i\n              class=\"el-icon-search el-input__icon\"\n              slot=\"suffix\"\n              @click=\"handleSelect\">\n            </i>\n            <template slot-scope=\"{ item }\">\n              <div style=\"display: flex;justify-content: space-between;\">\n                <span @click=\"selectHouse(item)\" class=\"name\">{{ item.houseCode }}</span>\n                <span @click=\"selectHouse(item)\" class=\"addr\">{{ item.id }}</span>\n              </div>\n            </template>\n          </el-autocomplete>\n        </el-form-item>\n        <el-form-item label=\"锁ID\" prop=\"lockId\">\n          <el-input v-model=\"form.lockId\" placeholder=\"请输入锁ID\" />\n        </el-form-item>\n        <el-form-item label=\"锁状态\">\n          <el-radio-group v-model=\"form.status\">\n            <el-radio\n              v-for=\"dict in dict.type.house_auth_status\"\n              :key=\"dict.value\"\n:label=\"parseInt(dict.value)\"\n            >{{dict.label}}</el-radio>\n          </el-radio-group>\n        </el-form-item>\n        <el-form-item label=\"锁密码\" prop=\"password\">\n          <el-input v-model=\"form.password\" placeholder=\"请输入锁密码\" />\n        </el-form-item>\n        <el-form-item label=\"账号\" prop=\"account\">\n          <el-input v-model=\"form.account\" placeholder=\"请输入账号\" />\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <