ol>\n    </el-row>\n  </div>\n</template>\n\n<script>\nimport userAvatar from \"./userAvatar\";\nimport userInfo from \"./userInfo\";\nimport resetPwd from \"./resetPwd\";\nimport { getUserProfile } from \"@/api/system/user\";\n\nexport default {\n  name: \"Profile\",\n  components: { userAvatar, userInfo, resetPwd },\n  data() {\n    return {\n      user: {},\n      roleGroup: {},\n      postGroup: {},\n      activeTab: \"userinfo\"\n    };\n  },\n  created() {\n    this.getUser();\n  },\n  methods: {\n    getUser() {\n      getUserProfile().then(response => {\n        this.user = response.data;\n        this.roleGroup = response.roleGroup;\n        this.postGroup = response.postGroup;\n      });\n    }\n  }\n};\n</script>\n"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6DA,OAAAA,UAAA;AACA,OAAAC,QAAA;AACA,OAAAC,QAAA;AACA,SAAAC,cAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAL,UAAA,EAAAA,UAAA;IAAAC,QAAA,EAAAA,QAAA;IAAAC,QAAA,EAAAA;EAAA;EACAI,IAAA,WAAAA,KAAA;IACA;MACAC,IAAA;MACAC,SAAA;MACAC,SAAA;MACAC,SAAA;IACA;EACA;EACAC,OAAA,WAAAA,QAAA;IACA,KAAAC,OAAA;EACA;EACAC,OAAA;IACAD,OAAA,WAAAA,QAAA;MAAA,IAAAE,KAAA;MACAX,cAAA,GAAAY,IAAA,WAAAC,QAAA;QACAF,KAAA,CAAAP,IAAA,GAAAS,QAAA,CAAAV,IAAA;QACAQ,KAAA,CAAAN,SAAA,GAAAQ,QAAA,CAAAR,SAAA;QACAM,KAAA,CAAAL,SAAA,GAAAO,QAAA,CAAAP,SAAA;MACA;IACA;EACA;AACA"}]}